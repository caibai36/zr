# See more detail at ../../timit/s0/exp/zr19/data/
mkdir -p exp/results/hybrid_onehot
head eval/abx/result/exp/hybrid_onehot/ce/test.vtln.deltas*/kl/abx.txt | awk 'BEGIN {printf "Method\tABX_average\n"} /abx\.txt/ {printf gensub(/.*\/(.*)\/(.*).post\/(cos|edit|kl)\/abx\.txt/, "\\2_\\3", "g", $2)"\t"} /average/ {print $5"\t"}' | sort -k1.27,1.28 -k1.36,1.37g | tee exp/results/hybrid_onehot/abx_kl.txt
head eval/abx/result/exp/hybrid_onehot/ce/test.vtln.deltas*/edit/abx.txt | awk 'BEGIN {printf "Method\tABX_average\n"} /abx\.txt/ {printf gensub(/.*\/(.*)\/(.*).post\/(cos|edit|kl)\/abx\.txt/, "\\2_\\3", "g", $2)"\t"} /average/ {print $5"\t"}' | sort -k1.27,1.28 -k1.36,1.37g | tee exp/results/hybrid_onehot/abx_edit.txt
head eval/abx/result/exp/hybrid_onehot/ce/test.vtln.deltas*/cos/abx.txt | awk 'BEGIN {printf "Method\tABX_average\n"} /abx\.txt/ {printf gensub(/.*\/(.*)\/(.*).post\/(cos|edit|kl)\/abx\.txt/, "\\2_\\3", "g", $2)"\t"} /average/ {print $5"\t"}' | sort -k1.27,1.28 -k1.36,1.37g | tee exp/results/hybrid_onehot/abx_cos.txt
